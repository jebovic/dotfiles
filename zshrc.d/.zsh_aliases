alias ..="cd .."
alias ...="cd ../../"
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ll="ls -lah --color=auto"
alias l="ll"
alias ap2="sudo service apache2"
alias scrl="screen -ls"

#SYMFONY
alias console="php app/console"
alias router="php app/console router:debug --env=prod"

#Tmux
alias session="tmuxifier load-session"
alias window="tmuxifier load-window"

# k8s
alias k="kubectl"
alias ka="for file in $(ls); do k apply -f $file; done"
alias kd="for file in $(ls); do k delete -f $file; done"

